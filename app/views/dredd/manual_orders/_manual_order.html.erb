<tr>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.print_code %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order&.full_name %>
    <% end %>
  </td>
  <td class="flex font-medium justify-center py-4 whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.phone_number %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.app_contact %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.price_for_modeling %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.price_for_printing %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.count %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.total_price %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.prepaid_expense %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.status %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= manual_order.deadline&.strftime("%F") %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap" scope="row">
    <%= link_to dredd_manual_order_path(manual_order), class: "text-black" do %>
      <%= l(manual_order.created_at, format: :long)  %>
    <% end %>
  </td>
  <td class="px-6 py-4 font-medium whitespace-nowrap space-x-4 flex" scope="row">
    <div class="text-black bg-primary p-1.5 rounded-2xl text-black">
      EDIT
    </div>
    <div class= "text-black bg-danger p-1.5 rounded-2xl text-black">
      DESTROY
    </div>
  </td>
