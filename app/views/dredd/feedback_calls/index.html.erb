<div class="lg:px-24 max-w-screen-2xl mt-16 mx-auto w-full">
  <table>
    <thead class="text-gray-700 bg-gray-50">
      <tr class="text-base">
        <th class="px-6 py-3" scope="col"><%= t("dredd.feedback_calls.id") %></th>
        <th class="px-6 py-3" scope="col"><%= t("dredd.feedback_calls.phone_number") %></th>
        <th class="px-6 py-3" scope="col"><%= t("dredd.feedback_calls.processed") %></th>
        <th class="px-6 py-3" scope="col"><%= t("dredd.feedback_calls.created_at") %></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: "dredd/feedback_calls/feedback_call", collection: @feedback_calls %>
    </tbody>
  </table>
  <%= pagy_nav(@pagy).html_safe if @pagy.pages > 1 %>
</div>
