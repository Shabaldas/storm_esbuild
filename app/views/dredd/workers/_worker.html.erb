<%= turbo_frame_tag worker do  %>
  <%= worker.id %>
  <%= worker&.full_name %>
  <%= worker.nickname %>
  <%= l(worker.created_at, format: :long)  %>
  <%= button_to "Delete",
  dredd_worker_path(worker),
  method: :delete,
  class: "btn btn--light" %>
  <%= link_to "Edit",
              edit_dredd_worker_path(worker),
              class: "btn btn--light" %>
              <br/>
<% end %>
